{
  "name": "co2monitor",
  "version": "1.0.0",
  "description": "Application for CO2 monitoring.",
  "main": "start.js",
  "engines": {
    "node": ">=7.0.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "highcharts": "^5.0.12",
    "serialport": "^4.0.7",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "clean-webpack-plugin": "^0.1.13",
    "webpack": "^1.13.2"
  },
  "scripts": {
    "start": "babel-node start.js --presets env",
